{
  "name": "storybook-7-vue-3-sample",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "latest": "yarn upgrade --latest",
    "dev": "vite --host",
    "preview": "vite preview",
    "build": "vue-tsc && vite build",
    "format": "yarn format:prettier && yarn format:lint",
    "format:lint": "eslint --fix src/*.{ts,vue} && eslint --fix src/**/*.{ts,vue}",
    "format:prettier": "prettier -w src/*.{ts,vue} && prettier -w src/**/*.{ts,vue}",
    "storybook:start": "storybook dev --port 6006",
    "storybook:test": "test-storybook --url http://localhost:6006"
  },
  "dependencies": {
    "pinia": "^2.1.7",
    "vue": "^3.3.12",
    "vue-i18n": "9",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^7.6.5",
    "@storybook/addon-backgrounds": "^7.6.5",
    "@storybook/addon-controls": "7.6.5",
    "@storybook/addon-docs": "7.6.5",
    "@storybook/addon-interactions": "7.6.5",
    "@storybook/addon-measure": "7.6.5",
    "@storybook/addon-outline": "^7.6.5",
    "@storybook/addon-toolbars": "7.6.5",
    "@storybook/addon-viewport": "^7.6.5",
    "@storybook/jest": "^0.2.3",
    "@storybook/test-runner": "^0.16.0",
    "@storybook/testing-library": "^0.2.2",
    "@storybook/vue3-vite": "^7.6.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-markdown": "^3.0.1",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-vue": "^9.19.2",
    "prettier": "^3.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "7.6.5",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vue-tsc": "^1.8.25"
  }
}
